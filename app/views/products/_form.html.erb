<%= render 'layouts/errors', :object => @product %>

<%= form_for @product do |f| %>
  <div class="col s6">

   <%= f.label :name %>
   <%= f.text_field :name %>

   <%= f.label :cost %>
   <%= f.text_field :cost %>

   <%= f.label :origin %>
   <%= f.text_field :origin %>

   <%= f.label "Enter Image URL"%>
   <%= f.text_field :img %>

   <%= f.submit class: "btn link-btn" %>

  </div>
 <% end %>
